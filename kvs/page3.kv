<BoxLayoutWidget>:
    canvas:
        Color:
            rgba: [1, 1, 1, 1]
        Rectangle:
            size: self.width*0.2, self.height*0.15
            pos: self.x+10, self.y+10
            source: "./images/shan.jpg"

        Ellipse:
            size: self.width*0.2, self.height*0.15
            pos: self.x+100, self.y+100

        Ellipse:
            size: self.width*0.2, self.height*0.15
            pos: self.x+200, self.y+200
            angle_start: 120
            angle_end: 420

        Ellipse:
            size: self.width*0.2, self.height*0.15
            pos: self.x+300, self.y+300
            angle_start: 120
            angle_end: 60

        Ellipse:
            size: self.width*0.1, self.height*0.05
            pos: self.x+550, self.y+250
            segments: 3

        Triangle:
            points: 110,350,  240,380,  280,300

        Quad:
            points: 340,250,  640,280,  480,500, 380,520
